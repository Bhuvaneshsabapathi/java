import java.util.Scanner;
class year{
    public static void main(String args[]){
        Scanner in=new Scanner(System.in);
        System.out.println("Enter the Year:");
        int n=in.nextInt();
        if(n%4==0){
            System.out.println("Leap Year");
        }else{
            System.out.println("Not Leap Year");
        }
    }
}
