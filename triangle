import java.util.Scanner;
class triangle{
    public static void main(String args[]){
        Scanner in=new Scanner(System.in);
        int a=in.nextInt();
        int b=in.nextInt();
        int c=in.nextInt();
        if(a+b>c && b+c>a && c+a>b){
            System.out.println("It will form a Triangle");
        }else{
            System.out.println("It not form a Triangle");
        }
    }
}
